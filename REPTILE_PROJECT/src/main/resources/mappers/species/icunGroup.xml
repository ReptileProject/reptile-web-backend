<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.reptile.species.model.dao.IcunGroupDao">
	<!--  1. 단일 그룹 상세 조회 -->
	<select id="searchIcunGroupDetail" resultType="com.reptile.species.model.IcunGroup">
	SELECT *
	FROM icun_group
	WHERE #{icunId}
	</select>
	
	<!--  2. 그룹 전체 조회 -->
	<select id="searchAll" resultType="com.reptile.species.model.IcunGroup">
	SELECT *
	FROM icun_group
	</select>
</mapper>